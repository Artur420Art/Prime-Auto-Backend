{
  "version": 2,
  "builds": [
    {
      "src": "src/api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/docs",
      "dest": "src/api/index.ts"
    },
    {
      "src": "/docs/(.*)",
      "dest": "src/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "src/api/index.ts"
    }
  ]
}
